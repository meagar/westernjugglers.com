<!DOCTYPE html>
<html>
<head>
	<title>UWO Juggling Club</title>
	
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>

<body>

	<div id="wrapper">

		<header>
		<h1 id="header">UWO Juggling Club</h1>

		<div id="up-coming">
			<p class="meeting">
				Next Meeting: Monday, July 29th, in the UCC (<a href="#">See All</a>)	
			</p>

			<p class="event">
				Next Event: <a href="#">RIT Spring Juggle-in</a>
			</p>
			<div style="clear:both"></div>	
		</div>
			
		<nav id="main">
			<%= top_nav %>
		</nav>

		</header>

		<section id="content">
			<% if notice %>
				<p id="notice"><%= notice %></p>
			<% end %>
			<%= yield %>
		</section>

	</div>
</body>
</html>
